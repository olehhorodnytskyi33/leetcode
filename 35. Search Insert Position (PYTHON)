class Solution:
    def searchInsert(self, nums: List[int], target: int) -> int:
        
        n = len(nums)
        
        if n == 0:
            
            return 0
        
        l = 0
        r = n
        
        while l < r:
            
            m = (l + r) // 2
            
            if nums[m] == target:
                return m
            elif nums[m] < target:
                l = m + 1
            else:
                r = m
                
        return l
